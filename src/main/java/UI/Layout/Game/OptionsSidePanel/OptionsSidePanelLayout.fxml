<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" prefHeight="485.0" prefWidth="339.0" spacing="15.0" styleClass="optionspanel" stylesheets="@optionssidepanel.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="SaveButton" mnemonicParsing="false" onAction="#Save" prefHeight="60.0" prefWidth="218.0" styleClass="savebutton" stylesheets="@optionssidepanel.css" textFill="TRANSPARENT">
            <VBox.margin>
                <Insets top="50.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="MenuButton" mnemonicParsing="false" onAction="#Menu" prefHeight="60.0" prefWidth="218.0" styleClass="menubutton" stylesheets="@optionssidepanel.css" textFill="TRANSPARENT">
            <VBox.margin>
                <Insets />
            </VBox.margin>
      </Button>
        <Button fx:id="ExitButton" mnemonicParsing="false" onAction="#Exit" prefHeight="60.0" prefWidth="218.0" styleClass="exitbutton" stylesheets="@optionssidepanel.css" textFill="TRANSPARENT">
            <VBox.margin>
                <Insets />
            </VBox.margin>
      </Button>
        <Button fx:id="CancelButton" mnemonicParsing="false" onAction="#Cancel" prefHeight="60.0" prefWidth="218.0" styleClass="cancelbutton" stylesheets="@optionssidepanel.css" textFill="TRANSPARENT" VBox.vgrow="NEVER">
            <VBox.margin>
                <Insets top="80.0" />
            </VBox.margin>
      </Button>
    </children>
</VBox>
